hoborg.data <- read.csv('manny-data.csv')
plot(hoborg.data$Fog)
plot(hoborg.data$Kincaid)
plot(hoborg.data$Flesch)
plot(hoborg.data$Kincaid)
plot(hoborg.data$Kincaid,lty=1)
